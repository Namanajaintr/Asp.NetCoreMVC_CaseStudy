@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center ">
    <h1 class="display-4 mt-5">Welcome To Dictionary</h1>
    <div id="carouselExampleIndicators"
         class="carousel slide"
         data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators"
                data-slide-to="0"
                class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-75 h-25"
                     src="https://files.realpython.com/media/Dictionaries-in-Python_Watermarked.3656a2293c00.jpg"
                     alt="First slide" />
            </div>
            <div class="carousel-item">
                <img class="d-block w-75 h-25"
                     src="https://www.pythonistacafe.com/static/img/pythonistacafe-header.jpg"
                     alt="Second slide" />
            </div>
            <div class="carousel-item">
                <img class="d-block w-75 h-25"
                     src="https://files.realpython.com/media/Creating-Custom-Dictionaries-in-Python_Watermarked.5632420c4854.jpg"
                     alt="Third slide" />
            </div>
        </div>
        <a class="carousel-control-prev"
           href="#carouselExampleIndicators"
           role="button"
           data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next"
           href="#carouselExampleIndicators"
           role="button"
           data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    <p>Click Here<a href="https://www.dictionary.com/">LEARN ABOUT THE ENGLISH LANGUAGE, WORD LISTS & FLASHCARDS</a>.</p>

</div>


